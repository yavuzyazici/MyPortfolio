@model List<Yavuz_Selim_Yazıcı.DAL.Entities.Experience>

<div class="row about-content about-content--timeline">

    <div class="col-full text-center">
        <h3>Kariyer.</h3>
    </div>

    @foreach (var item in Model)
    {
        <div class="col-six tab-full left">
            <div class="timeline">
                <div class="timeline__block">
                    <div class="timeline__bullet"></div>
                    <div class="timeline__header">
                        <p class="timeline__timeframe">@item.Date@*Şubat 2024 - Present*@</p>
                        <h3>@item.Head@*Geleceğini Kodla*@</h3>
                        <h5>@item.Title@*Volunteer*@</h5>
                    </div>
                    <div class="timeline__desc">
                        <p>@item.Description@*Geleceğini Kodla projesinde Proje Yönetimi, Grafik Tasarım, Wordpress Site gibi işleri yapıyorum*@</p>
                    </div>
                </div>
            </div> <!-- end timeline -->
        </div> <!-- end left -->
    }
